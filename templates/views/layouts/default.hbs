<!DOCTYPE html>
<html>
	<head>
		{{#if data.project}}
		    {{#if data.project.meta.title}}
		        <title>{{data.project.meta.title}}</title>
		        <meta property="og:title" content="{{data.project.meta.title}}">
		        <meta name="twitter:title" content="{{data.project.meta.title}}">
		    {{else}}
		        <title>{{data.project.title}}</title>
		        <meta property="og:title" content="{{data.project.title}}">
		        <meta name="twitter:title" content="{{data.project.title}}">
		    {{/if}}
		    {{#if data.project.meta.description}}
		        <meta name="description" content="{{data.project.meta.description}}">
		        <meta property="og:description" content="{{data.project.meta.description}}">
		        <meta name="twitter:description" content="{{data.project.meta.description}}">
		    {{else}}
		        <meta name="description" content="{{data.project.content.brief}}">
		        <meta property="og:description" content="{{data.project.content.brief}}">
		        <meta name="twitter:description" content="{{data.project.content.brief}}">
		    {{/if}}
		    <meta property="og:type" content="website">
		    <meta name="twitter:card" content="summary_large_image">
		    <meta property="og:image" content="{{data.project.image.url}}">
		    <meta name="twitter:image:src" content="{{data.project.image.url}}">
		    <meta property="og:url" content="{{data.project.fullProjectUrl}}">
		    <meta name="twitter:url" content="{{data.project.fullProjectUrl}}">
		    <meta property="article:published_time" content="{{data.project.publishedDate}}">
		    {{# each data.project.categories}}
		      <meta property="article:tag" content="{{name}}">
		    {{/each}}
		    <meta name="keywords" content="sidechain, studio, Melbourne, design, web-design, digital, {{# each data.project.categories}}{{name}}, {{/each}}">    
		{{else}}
		    <title>sidechain | studio</title>
		    <meta name="description" content="Sidechain is a digitally-focussed design, content and brand strategy studio based in Oceania.">          
		    <meta property="og:image" content="{{baseUrl}}/images/logo.jpg">
		    <meta name="twitter:image:src" content="{{baseUrl}}/images/logo.jpg">
		    <meta name="keywords" content="sidechain, melbourne, design, web-design, digital, touchdesigner">
		{{/if}}    

