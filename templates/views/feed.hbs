<div class="outer-container gallery-cont">
	<div class="container page-nav-cont">
		<h5 class="page-title">Journal - <i>A collection of things we find inspiring</i></h5>
	</div>
	<div class="container">
		{{#if feeds}}
			<div class="row gallery-images">
				<div class="col-xs-12 feed-col">
					<div class="grid masonry" id="grid">
					{{#each feeds}}
						{{#each images}}	
							<div class="item">
								<a href="{{cloudinaryUrl crop='fit'}}" data-lightbox="roadtrip" data-title="{{../url}}" data-alt="{{../content.brief}}">
									<img src="{{cloudinaryUrl crop='fit'}}">
								</a>
							</div>
						{{/each}}	
					{{/each}}
					</div>			
				</div>
			</div>	
		{{else}}
			<h3 class="text-muted">There are no image galleries yet.</h3>
		{{/if}}
	</div>
</div>